<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SplashActivity">

    <ImageView
        android:id="@+id/logo_splash_image_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/areas" />

    <TextView
        android:id="@+id/title_splash_text_view"
        style="@style/textViewSplashLogo"
        android:layout_width="0dp"
        android:layout_height="85dp"
        android:text="@string/app_name"
        app:layout_constraintBottom_toBottomOf="@+id/logo_splash_image_view"
        app:layout_constraintEnd_toEndOf="@+id/logo_splash_image_view"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="@+id/logo_splash_image_view"
        app:layout_constraintTop_toTopOf="@+id/logo_splash_image_view"
        app:layout_constraintVertical_bias="0.152"
        tools:ignore="TextContrastCheck" />

    <TextView
        android:id="@+id/title_splash_text_view2"
        style="@style/textViewSplashLogo1"
        android:layout_width="177dp"
        android:layout_height="72dp"
        android:text="@string/app_name"
        app:layout_constraintBottom_toBottomOf="@+id/title_splash_text_view"
        app:layout_constraintEnd_toEndOf="@+id/title_splash_text_view"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/title_splash_text_view"
        app:layout_constraintVertical_bias="0.0" />
</androidx.constraintlayout.widget.ConstraintLayout>